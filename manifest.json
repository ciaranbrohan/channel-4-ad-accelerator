{
    "name": "C4 Ad-accelerator",
    "version": "0.0.2",
    "description": "Speeds up annoying channel 4 and rte player ads.",
    "background": {
      "service_worker": "service_worker.js"
    },
    "permissions": ["webNavigation", "scripting"],
    "manifest_version": 3,
    "host_permissions": ["https://*.channel4.com/*" , "https://*.rte.ie/*"],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }